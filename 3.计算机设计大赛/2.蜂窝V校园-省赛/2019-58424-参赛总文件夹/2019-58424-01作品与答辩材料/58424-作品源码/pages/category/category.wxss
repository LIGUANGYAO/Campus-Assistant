page{
  background-color: white;
}
.list{
  border-top: 1px solid rgb(226, 226, 226);
}
.hide-tip{
  width: 100%;
  height: 100px;
  font-size: 30px;
  font-weight: 700;
  line-height: 100px;
  text-align: center;
  color: rgb(175, 175, 175);
}
.hide-con{
  width: 95%;
  font-size: 12px;
  margin: 0 auto;
  color: rgb(189, 189, 189);
}
.hide-image{
  width: 96%;
  height: 400rpx;
  margin: 10rpx auto;
  border-radius: 10rpx;
  margin-top: 10px;
  box-shadow: 0 0 15rpx 0 rgb(190, 190, 190);
}