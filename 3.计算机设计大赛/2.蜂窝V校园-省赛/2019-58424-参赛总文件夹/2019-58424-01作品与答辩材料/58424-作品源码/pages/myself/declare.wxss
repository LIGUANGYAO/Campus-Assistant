/* pages/myself/declare.wxss */
.demo{
  width: 90%;
  height: 100%;
  margin: 20rpx auto;
  background-color: white;
  box-shadow: 0 0 3px 0 rgb(150, 150, 150);
  padding-left: 3%;
  padding-right: 3%;
  border-radius: 5rpx;
  padding-bottom: 20rpx;
}
.tip{
  width: 100%;
  height: 100rpx;
  font-size: 18px;
  font-weight: 700;
  line-height: 120rpx;
  border-bottom: 1px solid rgb(224, 224, 224);
}
.del-tip{
  width: 100%;
  height: 100rpx;
  font-size: 20px;
  font-weight: 700;
  margin-top: 30rpx;
  color: rgb(87, 87, 87);
  line-height: 100rpx;
}
.del-text{
  width: 100%;
  margin: 0 auto;
  font-size: 16px;
  color: rgb(87, 87, 87);
  margin-top: 15rpx;
}